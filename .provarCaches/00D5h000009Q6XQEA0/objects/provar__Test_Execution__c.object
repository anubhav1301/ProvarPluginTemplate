        <deprecated>false</deprecated>
        <formula>AND(
    NOT(ISBLANK(provar__Start_Date_Time__c)), 
    NOT(ISBLANK(provar__End_Date_Time__c)), 
    provar__Duration__c &gt; provar__Test_Case__r.provar__Maximum_Execution_Time__c
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Duration Is Above Maximum</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
