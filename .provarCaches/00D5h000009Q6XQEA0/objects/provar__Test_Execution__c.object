    <validationRules>
        <fullName>provar__Start_date_must_be_before_end_date</fullName>
        <active>true</active>
        <description>The start date time for a test execution must occur before the end date time, when it is populated</description>
        <errorConditionFormula>NOT(ISNULL(provar__Start_Date_Time__c) || ISNULL(provar__End_Date_Time__c))&amp;&amp;
provar__Start_Date_Time__c &gt; provar__End_Date_Time__c</errorConditionFormula>
        <errorDisplayField>provar__End_Date_Time__c</errorDisplayField>
        <errorMessage>The end date time cannot occur before the start date time</errorMessage>
    </validationRules>
