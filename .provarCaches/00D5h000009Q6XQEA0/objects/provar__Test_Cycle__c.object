        <fullName>provar__Pass_Rate__c</fullName>
        <deprecated>false</deprecated>
        <description>Overall pass rate for all the tests executed in this suite. Useful for test reports and averaging pass rate or measuring over time without needing to include the Test Case Results.</description>
        <formula>IF(provar__Total_Tests_Executed__c &gt; 0, 
provar__Total_Passed__c / provar__Total_Tests_Executed__c, 
0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Pass Rate</label>
        <precision>18</precision>
        <required>false</required>
        <scale>1</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Percent</type>
    </fields>
    <fields>
        <fullName>provar__Progress__c</fullName>
        <deprecated>false</deprecated>
        <formula>IF(provar__Total_Test_Executions__c &gt; 0,
(provar__Total_Passed__c + provar__Total_Failed__c + provar__Total_Skipped__c) / provar__Total_Test_Executions__c,
0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Progress</label>
        <precision>18</precision>
        <required>false</required>
        <scale>1</scale>
