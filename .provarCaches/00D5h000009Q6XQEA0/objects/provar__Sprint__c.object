    <validationRules>
        <fullName>provar__Move_Active_to_Closed_Status</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISCHANGED(provar__Status__c),
ISPICKVAL(PRIORVALUE(provar__Status__c), &quot;Active&quot;),
NOT(ISPICKVAL(provar__Status__c, &quot;Closed&quot;))
)</errorConditionFormula>
        <errorMessage>Active status can only move to Closed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>provar__Move_Future_to_Active_Status</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISCHANGED(provar__Status__c),
ISPICKVAL(PRIORVALUE(provar__Status__c), &quot;Future&quot;),
NOT(ISPICKVAL(provar__Status__c, &quot;Active&quot;))
)</errorConditionFormula>
        <errorMessage>Future status can only move to Active.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>provar__Start_and_End_Date_Are_Required</fullName>
        <active>true</active>
        <errorConditionFormula>(ISPICKVAL( provar__Status__c , &apos;Active&apos;) ||  ISPICKVAL( provar__Status__c , &apos;Closed&apos;)) &amp;&amp; (ISNULL( provar__Start_Date__c ) || ISNULL( provar__End_Date__c ))</errorConditionFormula>
        <errorMessage>To set the sprint as Active or Closed, please fill in the Start and End dates.</errorMessage>
    </validationRules>
