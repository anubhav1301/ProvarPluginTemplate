    <validationRules>
        <fullName>provar__Slack_Webhook_Is_Required_When_Enabled</fullName>
        <active>true</active>
        <errorConditionFormula>PICKLISTCOUNT(provar__Slack_Alerts__c) &gt; 0 &amp;&amp;  provar__Slack_Webhook__c = NULL</errorConditionFormula>
        <errorMessage>Please, select a Slack Webhook.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>provar__Test_Type_required_to_create_Test_Cases</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( provar__Missing_Test_Case_Strategy__c , &apos;Create&apos;) &amp;&amp; ISBLANK(TEXT(provar__Test_Type__c))</errorConditionFormula>
        <errorMessage>Test Type is required when Missing Test Case Strategy is set to &apos;Create&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>provar__Time_is_required_when_frequency_is_Daily</fullName>
        <active>true</active>
        <description>Time must be defined when the frequency is not &quot;One-off&quot;.</description>
        <errorConditionFormula>NOT(ISPICKVAL(provar__Frequency__c, &apos;One-off&apos;)) &amp;&amp; ISBLANK( provar__Time__c)</errorConditionFormula>
        <errorMessage>Time must be defined when the frequency is not &quot;One-off&quot;.</errorMessage>
    </validationRules>
